  {
    "name": "String Extension",
    "version": "1.0",
    "description": "extension built to store a string",
    "permissions": ["activeTab", "declarativeContent", "storage"],
    "background": {
      "scripts": ["scripts.js"],
      "persistent": false
    },
    "page_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "activeLogo.png"
      }
    },
    "icons": {
      "16": "activeLogo.png"
    },
    "manifest_version": 2
  }
